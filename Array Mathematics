import numpy

n,m=input().split()
l1=[]
l2=[]
for i in range(int(n)):
    l1.append(list(map(int,input().split())))

for i in range(int(n)):
    l2.append(list(map(int,input().split())))

a=numpy.array(l1)
b=numpy.array(l2)
print(a+b,a-b,a*b,a//b,a%b,a**b,sep="\n")

